# Environment variables - Doppler will substitute ${VAR} placeholders
# Run: doppler secrets substitute .env.example > .env

# GitHub PAT (optional - if not set, web login will be used)
GH_TOKEN={{.GH_TOKEN}}

# Tailscale auth key (required for phase2)
TAILSCALE_AUTHKEY={{.TAILSCALE_AUTHKEY}}

# Tinybird credentials (optional - if not set, web login will be used)
TINYBIRD_HOST={{.TINYBIRD_HOST}}
TINYBIRD_TOKEN={{.TINYBIRD_TOKEN}}
